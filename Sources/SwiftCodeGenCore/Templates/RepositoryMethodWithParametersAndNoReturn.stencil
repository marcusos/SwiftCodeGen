{% if method.description %}/**
    {{ method.description }}
    */{% endif %}
    func {{ method.name }}(headers: [String: String]?, parameters: {{ method.targetName }}.Parameters) -> Completable {
        return self.provider
            .rx.request(.init({{ method.targetName }}(headers: headers, parameters: parameters)))
            .asObservable()
            .ignoreElements()
    }